<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
    
    <g:HTMLPanel>
        <table>
            <tr>
                <td><g:Label ui:field="loginLabel"/></td><td><g:TextBox ui:field="loginTextBox"/></td>
            </tr>
            <tr>
                <td><g:Label ui:field="passwordLabel"/></td><td><g:PasswordTextBox ui:field="passwordTextBox"/></td>
            </tr>
            <tr>
                <td><g:Label ui:field="confirmPasswordLabel"/></td><td><g:PasswordTextBox ui:field="confirmPasswordTextBox"/></td>
            </tr>
            <tr>
                <td/><td><g:Button ui:field="registerButton"/></td>
            </tr>
        </table>
    </g:HTMLPanel>
</ui:UiBinder> 